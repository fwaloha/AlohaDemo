<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.FragActivity">

    <fragment
        android:layout_width="0dp"
        android:id="@+id/frag_left"
        android:name="com.wf.aloha.ui.LeftFragment"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/frag_right"
        android:layout_height="match_parent">
    </fragment>
    <fragment
        android:layout_width="0dp"
        android:id="@+id/frag_right"
        android:name="com.wf.aloha.ui.RightFragment"
        app:layout_constraintLeft_toRightOf="@id/frag_left"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_height="match_parent"></fragment>
</android.support.constraint.ConstraintLayout>